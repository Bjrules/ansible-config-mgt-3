pipeline {
    agent any
    stage("Initial cleanup") {
          steps {
            dir("${WORKSPACE}") {
              deleteDir()
            }
          }
        }
    stages{
      stage('Build') {
        steps {
          script {
            sh 'echo "BUilders"'
        }
      }
    }
    stage('Cleanup') {
      steps {
          cleanWs()
        
      }
    }
    }
}